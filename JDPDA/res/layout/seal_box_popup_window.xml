<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:jd="http://schemas.android.com/apk/res/com.jd.a6i"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" 
    android:background="@drawable/jd_main_background">
   <include layout="@layout/action_bar_top_nav"/>
	<com.jd.a6i.view.TextEditView 
	    android:id="@+id/jdBoxNo"
	    style="@style/JDTextEditViewHorizontalTheme"
	    jd:labelText="@string/boxNo"
	    jd:labelPosition="left"/>
	<com.jd.a6i.view.TextEditView 
	    android:id="@+id/jdSealLabelNo"
	    style="@style/JDTextEditViewHorizontalTheme"
	    jd:labelText="@string/sealLabelNo"
	    labelPosition="left"/>
	<TextView 
	    style="@style/JDBaseTheme.JDTextViewTheme"
	    android:id="@+id/textOwerStation"
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content"/>
</LinearLayout>
